<!--index.wxml-->
<view class="container">
<view class="center">
  <view class="timeBlock">
    <text decode="true">ST\n{{ST}}</text>
  </view>
  <view class="timeBlock">
    <text decode="true">ET\n{{ET}}</text>
  </view>
</view>

<view class="center">
  <button class="mainBtn" bindtap="mainBtn">
    <text class="mainBtnTxt">{{mainBtnTxt}}</text>
  </button>
</view>
</view>
