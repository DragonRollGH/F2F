<van-calendar
  class = "tiled-calendar"
  poppable = "{{ false }}"
  show-title = "{{ false }}"
  show-subtitle = "{{ false }}"
  show-confirm = "{{ false }}"
  min-date = "{{ mindate }}"
  max-date = "{{ maxdate }}"
  default-date = "{{ defaultdate }}"
  formatter = "{{ dayFormatter }}"
  bind:confirm = "onCalendarConfirm"
/>