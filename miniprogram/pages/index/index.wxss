/**index.wxss**/

page {
  background: #f6f6f6;
}

.timeBlock {
  background: #c9c9c9;
  width: 40%;
  margin: auto;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}

.center {
  text-align: center;
  display: flex;
  justify-content: center;
}

.container {
  margin: 10%;
}

.mainBtn {
  margin-top: 20%;
  width: 400rpx !important;
  height: 400rpx;
  background-color: #07c160;
  justify-content: center;
  display: flex;
  color: #fff;
  border-radius: 200px;
  -webkit-tap-highlight-color: rgba(0,0,0,0);
}

.mainBtnTxt {
  font-size: larger;
  line-height :350rpx;
}