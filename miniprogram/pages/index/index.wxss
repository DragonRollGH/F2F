/**index.wxss**/

page {
  background: #f6f6f6;
}

.timeBlock {
  background: hsl(223, 33%, 80%);
  width: 40%;
  margin: auto;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}

.center {
  text-align: center;
  display: flex;
  justify-content: center;
}

.container {
  margin: 10%;
}

.mainBtn {
  margin-top: 10%;
  width: 400rpx !important;
  height: 400rpx;
  border-radius: 200rpx;
  background-color: rgb(0, 47, 167);
  justify-content: center;
  box-shadow: 10rpx 10rpx 10rpx #00000020;
}

.mainBtnTxt {
  font-size: larger;
  line-height :360rpx;
}

.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 40%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}